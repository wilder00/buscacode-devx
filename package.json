{
  "name": "buscacode-devx",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start --port 3000",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "lint": "eslint",
    "lint:fix": "eslint . -c eslint.config.mjs --fix",
    "lint:check": "eslint . -c eslint.config.mjs --no-fix",
    "format:fix": "prettier --ignore-path .prettierignore --write .",
    "format:check": "prettier --ignore-path .prettierignore --check .",
    "format:watch": "onchange \"**/*\" -- prettier --write --ignore-path .prettierignore --ignore-unknown {{changed}}",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "coverage:show": "http-server -c-1 -o ./coverage/index.html -p 6001",
    "coverage:show-lcov": "http-server -c-1 -o ./coverage/lcov-report/index.html -p 6001",
    "coverage:verify-lcov": "lcov-total ./coverage/lcov.info && lcov-total ./coverage/lcov.info --gte=50"
  },
  "dependencies": {
    "@docusaurus/core": "3.6.3",
    "@docusaurus/plugin-content-docs": "3.6.3",
    "@docusaurus/plugin-svgr": "3.6.1",
    "@docusaurus/preset-classic": "3.6.3",
    "@docusaurus/theme-live-codeblock": "3.6.3",
    "@docusaurus/theme-mermaid": "3.6.3",
    "@mdx-js/react": "3.1.0",
    "@types/node": "22.10.1",
    "@types/react-dom": "18.2.0",
    "@types/react": "18.2.0",
    "clsx": "2.1.1",
    "prism-react-renderer": "2.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rehype-katex": "7.0.1",
    "remark-math": "6.0.0",
    "remark-playground": "0.1.3",
    "typescript": "5.7.2"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.6.3",
    "@docusaurus/tsconfig": "3.6.3",
    "@docusaurus/types": "3.6.3",
    "@eslint/js": "9.16.0",
    "@types/css-modules": "1.0.5",
    "@vitest/coverage-istanbul": "2.1.6",
    "@vitest/coverage-v8": "2.1.6",
    "@vitest/eslint-plugin": "1.0.1",
    "eslint": "9.16.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "7.37.2",
    "globals": "15.12.0",
    "http-server": "14.1.1",
    "lcov-total": "2.1.1",
    "onchange": "7.1.0",
    "prettier": "3.4.1",
    "rimraf": "6.0.1",
    "tsup": "8.3.5",
    "typescript-eslint": "8.16.0",
    "vitest": "2.1.6"
  },
  "overrides": {
    "eslint": "9.16.0"
  },
  "resolutions": {
    "@types/react": "18.2.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  },
  "packageManager": "pnpm@9.1.1+sha512.14e915759c11f77eac07faba4d019c193ec8637229e62ec99eefb7cf3c3b75c64447882b7c485142451ee3a6b408059cdfb7b7fa0341b975f12d0f7629c71195"
}
